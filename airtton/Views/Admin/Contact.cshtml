@model airtton.ViewModel.ContactSummaryViewModel

@{
    ViewBag.Title = "Contact";
    Layout = "~/Views/Admin/Shared/_Layout.cshtml";
}

<div class="portlet box blue col-md-11">
    <div class="portlet-title">
        <div class="caption">
            <i class="fa fa-gift"></i> 联系方式
        </div>
        <div class="actions">
            <button class="btn btn-default btn-sm" id="contact_edit_btn" data-url="@Url.Action("EditContact", "Admin")">
                <i class="fa fa-pencil"></i> 编辑
            </button>
        </div>
    </div>
    <div class="portlet-body form">
        <form role="form" id="contact_portlet">
            @Html.Partial("Partial/_Contact", Model)
        </form>
    </div>

</div>



<script src="~/assets/js/admin/jquery.min.js" type="text/javascript"></script>
<script src="~/Content/Pages/Admin/handle-contact.js" type="text/javascript"></script>


